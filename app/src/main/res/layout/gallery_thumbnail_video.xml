<?xml version="1.0" encoding="utf-8"?>
<com.example.irfanfahmiwijaya.humassmartclickapp.helper.SquareLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="5dp"
    android:background="@android:color/black"
    android:orientation="vertical">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:id="@+id/kontencard">
            <VideoView
                android:id="@+id/thumbnailv"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:adjustViewBounds="true"
                android:scaleType="centerCrop"/>
</LinearLayout>
</com.example.irfanfahmiwijaya.humassmartclickapp.helper.SquareLayout>